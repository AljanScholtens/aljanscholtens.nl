}

@media (min-width: 750px) {

.c-card__image {
    margin-right: 32px;
    flex: 0 0 200px;
    height: 200px
}
  }

.c-card__image img {
    -o-object-fit: cover;
       object-fit: cover;
    display: block;
    width: 64px;
    height: 64px;
  }

@media (min-width: 750px) {

.c-card__image img {
      width: 200px;
      height: 200px
  }
    }

