/*------------------------------------*\
  #STYLES
\*------------------------------------*/

/*
 * #TOKENS
 */

/*------------------------------------*\
  #FONTS
\*------------------------------------*/

@font-face {
  font-family: "Sofia Pro";
  font-weight: 400;
  src: url("/fonts/sofia-pro-regular.woff2") format("woff2");
}

@font-face {
  font-family: "Sofia Pro";
  font-weight: 500;
  src: url("/fonts/sofia-pro-medium.woff2") format("woff2");
}

@font-face {
  font-family: "Sofia Pro";
  font-weight: 700;
  src: url("/fonts/sofia-pro-semibold.woff2") format("woff2");
}

@font-face {
  font-family: "Lora";
  font-weight: 400;
  src: url("/fonts/lora_regular.woff2") format("woff2");
}

@font-face {
  font-family: "Aqua";
  font-weight: 400;
  src: url("/fonts/aqua-regular.woff2") format("woff2");
}

@font-face {
  font-family: "Nanum Pen";
  font-weight: 400;
  src: url("/fonts/nanum-pen-regular.woff2") format("woff2");
}

/*------------------------------------*\
  #TOKENS
\*------------------------------------*/

/*
 * Most of the variables are based on some kind of percentages to maintain stability and consistency.
 * So --variable-10 is small or light and --variable-100 is big or dark.
 */

:root {
  /*
   * Typography
   */

  /* Fonts */
  --font-family-a: Sofia Pro, "Helvetica Neue", sans-serif;
  --font-family-b: Lora, Georgia, serif;
  --font-family-c: Nanum Pen, Lucida Handwriting;
  --font-family-d: Aqua, "Helvetica Neue", sans-serif;

  /* Legacy */
  --font-family-heading: var(--font-family-a);
  --font-family-body: var(--font-family-a);

  /* Set your font size */
  --font-size-base: 16px;

  --font-size-default: var(--font-size-30);
  --font-size-10: 12px; /* Used for small all caps labels */
  --font-size-20: 14px;
  --font-size-30: 17px;
  --font-size-40: 19px; /* Body */
  --font-size-50: 26px; /* Lead */
  --font-size-60: 30px; /* Heading */
  --font-size-65: 40px; /* Heading */
  --font-size-70: 5vw; /* Heading */

  /* Line-height */
  --line-height-default: var(--line-height-10);
  --line-height-10: 1;
  --line-height-20: 1.35em;
  --line-height-30: 1.65em;

  /* Letter spacing */
  --letter-spacing-default: var(--letter-spacing-10);
  --letter-spacing-10: 2px;

  /* Font weight */
  --font-weight-default: var(--font-weight-20);
  --font-weight-10: 300;
  --font-weight-20: 400;
  --font-weight-30: 500;

  /*
   * Spacing
   */

  /* Spacing system */
  --spacing: var(--space-default); /* Legacy */

  --space-default: var(--space-30);
  --space-10: 4px;
  --space-20: 8px;
  --space-25: 12px; /* Not default */
  --space-30: 16px;
  --space-40: 32px;
  --space-50: 64px;

  --space-stack-default: var(--space-stack-40);
  --space-stack-10: 0 0 4px 0;
  --space-stack-20: 0 0 8px 0;
  --space-stack-30: 0 0 16px 0;
  --space-stack-40: 0 0 32px 0;
  --space-stack-50: 0 0 64px 0;

  --space-inset-default: var(--space-inset-30);
  --space-inset-10: 4px 4px 0.1px 4px;
  --space-inset-20: 8px 8px 0.1px 8px;
  --space-inset-30: 16px 16px 0.1px 16px;
  --space-inset-40: 32px 32px 16px 32px;
  --space-inset-50: 64px 64px 48px 64px;

  --space-squish-10: 4px 8px;
  --space-squish-20: 8px 16px;
  --space-squish-25: 12px 24px; /* Not default */
  --space-squish-30: 16px 32px;

  /* Grid gutter */
  --gutter: 25px;

  /* Maximum width of the website */
  --max-width: 1240px;

  /*
   * Colors
   */

  /* Primary colors */
  --color-primary-10: #f2f2f7;
  --color-primary-30: #9c9af4;
  --color-primary-50: #523ef8;
  --color-primary-70: #3330a6;
  --color-primary-90: #121138;
  --color-primary-90a80: #12113880;

  /* Secondary colors */
  --color-secondary-10: #feecf2;
  --color-secondary-30: #ff6b99;
  --color-secondary-30a20: #ff6b9920
  --color-secondary-50: #ff3473;
  --color-secondary-70: #d31e56;

  /* Tertiary colors */
  --color-tertiary-10: #fff4ea;
  --color-tertiary-30: #ffd3a6;

  /* Success and positive colors */
  --color-success-50: #41d57d;
  --color-success-70: #21ab59;

  /* Alert en negative colors */
  --color-alert-50: var(--color-secondary-50);
  --color-alert-70: var(--color-secondary-70);

  /* Neutral colors */
  --color-neutral-0: #fff;
  --color-neutral-10: #fef9f2;
  --color-neutral-30: #d5d3ce;
  --color-neutral-50: #8f8d87;
  --color-neutral-60: #615f5b;
  --color-neutral-70: #3c3b38;
  --color-neutral-90: #201e1c;
  --color-neutral-100: #000;
  --color-neutral-100a80: #00000080;

  /*
   * Other
   */

  /* Rounded borders */
  --border-radius-default: var(--border-radius-10);
  --border-radius-10: 2px;
  --border-radius-20: 4px;
  --border-radius-50: 10rem;

  /* Shadows */
  --box-shadow-default: var(--box-shadow-10);
  --box-shadow-10: 0px 2px 10px color-mod(var(--color-neutral-100) alpha(10%));
  --box-shadow-30: 0px 10px 50px color-mod(var(--color-neutral-100) alpha(10%));

  /* Animation beziers */
  --bezier-default: var(--bezier-a);
  --bezier-a: cubic-bezier(0.86, 0, 0.07, 1);

  /* Layers */
  --layer-10: 10;
  --layer-20: 20;
  --layer-30: 30;
}

/* Breakpoints */

/**
 * #BASE
 */

/*------------------------------------*\
  #BASE-PAGE
\*------------------------------------*/

*,
*:after,
*:before {
  box-sizing: border-box;
}

html {
  background-color: var(--color-neutral-10);
  color: var(--color-neutral-50);
  font-weight: 400;
  font-size: var(--font-size-base);
  font-family: var(--font-family-body);
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizeLegibility;
  -webkit-text-size-adjust: 100%;
  height: 100%;
}

html.t-dark {
  background-color: var(--color-primary-90);
}

body {
  min-height: 100vh;
  height: 100%;
  margin: 0;
  line-height: var(--line-height-20);
  font-size: var(--font-size-default);
  line-height: var(--line-height-default);
}

/*------------------------------------*\
  #BASE-TYPE
\*------------------------------------*/

/*
 * Headings
 */

h1,
h2,
legend,
h3,
h4,
h5,
h6 {
  font-family: var(--font-family-heading);
  font-weight: 600;
  font-size: var(--font-size-50);
  margin: 0;
}

/*
 * Paragraphs has been moved entirely to the paragraph component
 */

/*
 * Links
 */

a {
  color: var(--color-secondary-50);
  text-decoration: none;
  font-weight: 500;
  cursor: pointer;
}

a:hover {
    color: var(--color-neutral-90);
  }

.u-content a {
  background-color: var(--color-secondary-10);
  padding: 0.2em 0.5rem;
}

.t-dark .u-content a {
  color: var(--color-secondary-30);
  background-color: var(--color-secondary-30a20);
}

.t-dark .u-content a:hover {
    color: var(--color-neutral-0);
  }

/*
 * Other
 */

small {
  font-family: var(--font-family-heading);
  color: var(--color-neutral-50);
  font-size: var(--font-size-20);
}

em {
  font-style: italic;
}

strong {
  font-weight: 700;
}

code,
pre {
  font-family: "courier new", monospace;
}

pre {
  margin: 0;
  overflow-x: auto;
}

abbr {
  border-bottom: 1px dotted var(--color-neutral-50);
}

del {
  text-decoration: line-through;
}

figure {
  margin: 0;
}

hr {
  background-color: transparent;
  border: solid var(--color-neutral-30);
  border-width: 1px 0 0;
  clear: both;
  margin: 2.5rem var(--space-default);
}

ul,
ol {
  margin: 0;
  padding: 0;
}

/**
 * #LAYOUT
 */

/*------------------------------------*\
  #LAYOUT-WRAPPERS
\*------------------------------------*/

/* All items with dark theme gets a white color */

.t-dark {
  color: var(--color-neutral-0);
}

/*
 * Get full width backgrounds around your content
 */

.l-wrapper {
  clear: both;
  position: relative;
}

.l-wrapper:first-of-type {
    padding-top: 76px;
  }

.l-wrapper:last-of-type {
    padding-bottom: 3.5rem;
  }

@media (min-width: 750px) {

.l-wrapper:last-of-type {
      padding-bottom: 0
  }
    }

.t-dark .l-wrapper {
  background: var(--color-primary-90);
}

/*
 * Options to change the layout of the wrapper
 */

.l-wrapper--full-height {
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  min-height: 80vmin;
}

/*
 * Set up different backgrounds
 */

.l-wrapper--neutral-10 {
  background: var(--color-neutral-10);
}

.l-wrapper--neutral-10-gradient {
  background-image: linear-gradient(
    140deg,
    var(--color-neutral-10),
    var(--color-neutral-0)
  );
}

.l-wrapper--neutral-70 {
  background: var(--color-neutral-90);
  color: var(--color-neutral-10);
}

.l-wrapper--neutral-70 a {
    color: var(--color-primary-30);
  }

/*------------------------------------*\
  #LAYOUT-CONTAINERS
\*------------------------------------*/

/*
 * Build up your container to get your website centered
 */

.l-container {
  margin: auto;
  width: 100%;
  max-width: var(--max-width);
  padding: var(--spacing) var(--spacing) 0.1px var(--spacing);
  position: relative;
}

@media (min-width: 750px) {

.l-container {
    padding: calc(var(--spacing) * 2);
    padding-bottom: var(--spacing)
}
  }

@media (min-width: 750px) {

.l-container {
    padding: calc(var(--spacing) * 5);
    padding-bottom: calc(var(--spacing) * 3)
}
  }

.l-container--full-width {
  max-width: none;
  margin: 0;
}

/*------------------------------------*\
  #LAYOUT-GRID
\*------------------------------------*/

/*
 * Add your own grid items
 */

.l-single {
  max-width: 62ch;
  width: 100%;
  margin: auto;
  clear: both;
}

.l-single-wide {
  max-width: 46rem;
  margin: auto;
  clear: both;
  position: relative;
}

.c-grid {
  display: grid;
  grid-gap: var(--space-40);
}

.c-grid--halves {
  grid-template-columns: 1fr 1fr;
}

/**
 * #PLUGINS
 */

/*! Flickity v2.2.1
https://flickity.metafizzy.co
---------------------------------------------- */

.flickity-enabled {
  position: relative;
}

.flickity-enabled:focus { outline: none; }

.flickity-viewport {
  overflow: hidden;
  position: relative;
  height: 100%;
}

.flickity-slider {
  position: absolute;
  width: 100%;
  height: 100%;
}

/* draggable */

.flickity-enabled.is-draggable {
  -webkit-tap-highlight-color: transparent;
  -webkit-user-select: none;
     -moz-user-select: none;
          user-select: none;
}

.flickity-enabled.is-draggable .flickity-viewport {
  cursor: move;
  cursor: grab;
}

.flickity-enabled.is-draggable .flickity-viewport.is-pointer-down {
  cursor: grabbing;
}

/* ---- flickity-button ---- */

.flickity-button {
  position: absolute;
  background: hsla(0, 0%, 100%, 0.75);
  border: none;
  color: #333;
}

.flickity-button:hover {
  background: white;
  cursor: pointer;
}

.flickity-button:focus {
  outline: none;
  box-shadow: 0 0 0 5px #19F;
}

.flickity-button:active {
  opacity: 0.6;
}

.flickity-button:disabled {
  opacity: 0.3;
  cursor: auto;
  /* prevent disabled button from capturing pointer up event. #716 */
  pointer-events: none;
}

.flickity-button-icon {
  fill: currentColor;
}

/* ---- previous/next buttons ---- */

.flickity-prev-next-button {
  top: 50%;
  width: 44px;
  height: 44px;
  border-radius: 50%;
  /* vertically center */
  transform: translateY(-50%);
}

.flickity-prev-next-button.previous { left: 10px; }

.flickity-prev-next-button.next { right: 10px; }

/* right to left */

.flickity-rtl .flickity-prev-next-button.previous {
  left: auto;
  right: 10px;
}

.flickity-rtl .flickity-prev-next-button.next {
  right: auto;
  left: 10px;
}

.flickity-prev-next-button .flickity-button-icon {
  position: absolute;
  left: 20%;
  top: 20%;
  width: 60%;
  height: 60%;
}

/* ---- page dots ---- */

.flickity-page-dots {
  position: absolute;
  width: 100%;
  bottom: -25px;
  padding: 0;
  margin: 0;
  list-style: none;
  text-align: center;
  line-height: 1;
}

.flickity-rtl .flickity-page-dots { direction: rtl; }

.flickity-page-dots .dot {
  display: inline-block;
  width: 10px;
  height: 10px;
  margin: 0 8px;
  background: #333;
  border-radius: 50%;
  opacity: 0.25;
  cursor: pointer;
}

.flickity-page-dots .dot.is-selected {
  opacity: 1;
}

/**
 * #COMPONENTS
 */

/*------------------------------------*\
  #COMPONENTS-AUTHOR
\*------------------------------------*/

.c-author {
  margin-bottom: var(--space-stack-40);
  padding-top: var(--space-40);
}

.c-author a {
    color: var(--color-neutral-70);
    text-decoration: none;
    background-color: transparent;
    padding: 0;
  }

.c-author figure {
    height: 4.5rem;
    width: 4.5rem;
    display: block;
    margin: auto;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
    border-radius: 50%;
    margin: var(--space-stack-20);
    margin-left: auto;
    margin-right: auto;
  }

.l-wrapper--neutral-70 .c-author a {
    color: var(--color-neutral-10);
  }

/*------------------------------------*\
  #COMPONENTS-BUTTONS
\*------------------------------------*/

.u-content .c-button,
.c-button {
  border: 0;
  border-radius: var(--border-radius-50);
  cursor: pointer;
  display: inline-block;
  font-family: var(--font-family-a);
  font-size: inherit;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-weight: 500;
  line-height: 1.5em;
  margin: 0;
  padding: 0.5rem 1.25rem;
  text-decoration: none;
  vertical-align: bottom;
  background: var(--color-primary-50);
  color: var(--color-neutral-0);
}

.u-content .c-button:hover, .c-button:hover {
    background: var(--color-primary-70);
    color: var(--color-neutral-0);
  }

.u-content .c-button:active,
  .u-content .c-button:focus,
  .c-button:active,
  .c-button:focus {
    box-shadow: inset 0 1px 3px var(--color-neutral-100a80);
  }

.c-button--small {
  padding: 0.5rem 0.9rem;
  line-height: 1;
  font-size: var(--font-size-20);
  border-radius: var(--border-radius-10);
}

.c-button--large {
  padding: 0.75rem 1.75rem;
}

.c-button--neutral {
  background: var(--color-neutral-10);
  color: var(--color-neutral-70);
}

.c-button--neutral:hover {
    background: var(--color-neutral-30);
    color: var(--color-neutral-70);
  }

.c-button--neutral:active,
  .c-button--neutral:focus {
    box-shadow: inset 0 1px 3px var(--color-neutral-100a80);
  }

.c-button--success {
  background: var(--color-success-50);
  color: var(--color-neutral-0);
}

.c-button--success:hover {
    background: var(--color-success-70);
    color: var(--color-neutral-0);
  }

.c-button--success:active,
  .c-button--success:focus {
    box-shadow: inset 0 1px 3px var(--color-neutral-100a80);
  }

.c-button--alert {
  background: var(--color-alert-50);
  color: var(--color-neutral-0);
}

.c-button--alert:hover {
    background: var(--color-alert-70);
    color: var(--color-neutral-0);
  }

.c-button--alert:active,
  .c-button--alert:focus {
    box-shadow: inset 0 1px 3px var(--color-neutral-100a80);
  }

.c-button--disabled {
  background: var(--color-neutral-30);
  color: var(--color-neutral-50);
  pointer-events: none;
}

/*------------------------------------*\
  #COMPONENTS-CARDS
\*------------------------------------*/

.c-card-grid {
}

.c-card {
