  color: var(--color-primary-50);
  line-height: var(--line-height-10);
}

.c-heading--10,
.u-content h3 {
  font-size: 22px;
  line-height: 1.35em;
}

.c-heading--30,
.u-content h2 {
  font-size: var(--font-size-60);
  line-height: 1.2em;
  font-family: var(--font-family-a);
  color: var(--color-primary-50);
  font-weight: 500;
}

.t-dark .c-heading--30,
.t-dark .u-content h2 {
  color: var(--color-primary-30);
}

.c-heading--50 {
  font-family: var(--font-family-b);
  font-size: 40px;
  font-weight: 300;
}

@media (min-width: 750px) {

.c-heading--50 {
    font-size: 8vw;
    margin: var(--space-stack-default)
}
  }

@media (min-width: 1000px) {

.c-heading--50 {
    font-size: var(--font-size-70)
}
  }

.t-dark .c-heading--50 {
  color: var(--color-primary-30);
}

@keyframes c-heading--animation {

  0% {
    opacity: 0;
    transform: translateX(-200px);
  }

  100% {
    opacity: 1;
    transform: translateX(0);
  }
}

@media (min-width: 750px) {

.c-heading--animation {
    opacity: 0;
    animation: c-heading--animation 600ms cubic-bezier(0, 0, 0, 1) forwards 200ms
}
  }

/*------------------------------------*\
  #COMPONENTS-HIGHLIGHT
\*------------------------------------*/

.c-highlight {
  display: block;
  text-align: center;
  line-height: 1.5em;
  font-size: var(--font-size-40);
  margin: var(--space-stack-default);
  font-style: italic;
}

@media (min-width: 750px) {

.c-highlight {
    font-size: var(--font-size-50);
    margin: var(--spacing) 0 calc(var(--spacing) * 2) 0
}
  }

/*------------------------------------*\
  #COMPONENTS-ICONS
\*------------------------------------*/

.c-icon {
  height: 24px;
  width: 24px;
  display: inline-block;
  fill: var(--color-neutral-70);
  vertical-align: top;
}

.c-icon--inline {
  margin: 0 0.25rem;
  position: relative;
  top: 0.25rem;
}

.c-icon--white {
  fill: var(--color-neutral-0);
}

/*------------------------------------*\
  #COMPONENTS-LIST
\*------------------------------------*/

.c-list,
.u-content ul {
  list-style: none;
  font-size: var(--font-size-40);
  color: var(--color-neutral-90);
  line-height: var(--line-height-30);
  margin: var(--space-stack-30);
}

.c-list li, .u-content ul li {
    padding-left: 2rem;
    position: relative;
  }

@media (min-width: 750px) {

.c-list li, .u-content ul li {
      padding-left: 3rem
  }
    }

.c-list li:before, .u-content ul li:before {
      position: absolute;
      left: 0.5rem;
      content: " ";
    }

@media (min-width: 750px) {

.c-list li:before, .u-content ul li:before {
        left: 1.5rem
    }
      }

.t-dark .c-list {
  color: var(--color-neutral-0);
}

ul.c-list li:before, .u-content ul li:before {
    top: 0.8rem;
    height: 6px;
    width: 6px;
    border-radius: 50%;
    background: var(--color-primary-50);
  }

.t-dark .c-list li:before,
.l-wrapper--neutral-70 .c-list li:before {
  background: var(--color-primary-30);
}

ol.c-list li, .u-content ol li {
    counter-increment: list__counter;
  }

ol.c-list li:before, .u-content ol li:before {
      content: counter(list__counter) ".";
    }

/*------------------------------------*\
  #COMPONENTS-LOGO_GRID
\*------------------------------------*/

.c-logo-grid {
  list-style: none;
  margin: var(--space-stack-default);
  display: grid;
  grid-template-columns: repeat(4, 1fr);
}

.c-logo-grid__item {
  padding: var(--space-default);
  display: flex;
}

.c-logo-grid__item img {
    margin: auto;
    max-width: 100%;
    mix-blend-mode: darken;
  }

.t-dark .c-logo-grid__item img {
  filter: invert(100%) grayscale(100%);
  mix-blend-mode: screen;
}

/*------------------------------------*\
  #COMPONENTS-MEDIA
\*------------------------------------*/

.c-media {
  margin: var(--space-stack-default);
  position: relative;
  width: 100%;
}

.c-media__image {
    display: block;
    max-width: 100%;
  }

.c-media__caption {
    color: var(--color-neutral-50);
    font-size: var(--font-size-20);
  }

.c-media--video {
  height: 0;
  padding-bottom: 56.25%;
  width: 100%;
}

.c-media--video iframe {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
  }

/*------------------------------------*\
  #COMPONENTS-MOBILE
\*------------------------------------*/

.c-navigation-mobile__wrapper {
  position: fixed;
  z-index: var(--layer-30);
  left: 6px;
  right: 6px;
  bottom: 0;
  font-weight: 600;
  font-family: var(--font-family-heading);
  background: color-mod(var(--color-primary-50) alpha(0.95));

  /* &:after {
    content: "";
    position: absolute;
    z-index: 1;
    top: 0;
    right: 0;
    bottom: 0;
    width: 1rem;
    background: linear-gradient(90deg, transparent, var(--color-primary-50));
  }

  &:before {
    content: "";
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    bottom: 0;
    width: 1rem;
    background: linear-gradient(-90deg, transparent, var(--color-primary-50));
  } */
}

.c-navigation-mobile {
  list-style: none;
  margin: 0;
  display: flex;
  flex-flow: row nowrap;

  overflow-x: auto;
  -webkit-overflow-scrolling: touch;
  white-space: nowrap;
}

@media (min-width: 750px) {

.c-navigation-mobile {
    display: none
}
  }

.c-navigation-mobile__item {
  float: left;
  position: relative;
  flex-grow: 1;
  text-align: center;
  /* font-size: var(--font-size-20); */
}

@media (min-width: 600px) {

.c-navigation-mobile__item {
    font-size: var(--font-size-30)
}
  }

.c-navigation-mobile__item a {
    color: var(--color-neutral-0);
    text-decoration: none;
    display: block;
    font-weight: 400;
    padding: 1rem 1rem;
  }

.c-navigation-mobile__item a:hover {
      color: var(--color-neutral-0);
    }

.c-navigation-mobile__item svg {
    width: 1rem;
    height: 1rem;
    position: relative;
    top: 3px;
    fill: var(--color-neutral-0);
  }

/*------------------------------------*\
  #COMPONENTS-NAVIGATION
\*------------------------------------*/

@keyframes c-navigation {
  0% {
    opacity: 0;
    transform: translateY(-10px);
  }

  100% {
    opacity: 1;
    transform: translateY(0);
  }
}

.c-navigation {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 16px;
  position: absolute;
  z-index: var(--layer-30);
  left: 0;
  right: 0;
  top: 0;
}

@media (min-width: 750px) {

.c-navigation {
    padding: 28px;
    justify-content: auto
}
  }

.t-dark .c-navigation .c-navigation__item.c-navigation__item--selected a {
    color: var(--color-neutral-0);
  }

/*
 * Logo
 */

.c-navigation__logo {
  display: block;
  text-decoration: none;
  font-weight: 400;
  font-size: 26px;
  color: var(--color-primary-70);
  font-family: Aqua;
}

@media (min-width: 750px) {

.c-navigation__logo {
    opacity: 0;
    padding: 16px;
    margin-right: 24px;
    animation: c-navigation 300ms cubic-bezier(0, 0, 0, 1) forwards 200ms
}
  }

.c-navigation__logo:hover {
    color: var(--color-primary-90);
  }

.t-dark .c-navigation__logo {
  color: var(--color-neutral-0);
}

.t-dark .c-navigation__logo:hover {
    color: color-mod(var(--color-neutral-0) alpha(80%));
  }

/*
 * Navigation items
 */

.c-navigation__items {
  list-style: none;
  margin: 0;
  flex: auto;
  display: none;
}

@media (min-width: 750px) {

.c-navigation__items {
    display: flex;
    gap: 16px
}
  }

@media (min-width: 750px) {

.c-navigation__item {
    opacity: 0;
    animation: c-navigation 300ms cubic-bezier(0, 0, 0, 1) forwards
}

    .c-navigation__item:nth-child(1) {
      animation-delay: 300ms;
    }

    .c-navigation__item:nth-child(2) {
      animation-delay: 350ms;
    }

    .c-navigation__item:nth-child(3) {
      animation-delay: 400ms;
    }

    .c-navigation__item:nth-child(4) {
      animation-delay: 450ms;
    }

    .c-navigation__item:nth-child(5) {
      animation-delay: 500ms;
    }

    .c-navigation__item:nth-child(6) {
      animation-delay: 550ms;
    }
  }

.c-navigation__item.c-navigation__item--selected a {
    color: var(--color-neutral-90);
  }

.c-navigation__item a {
    color: var(--color-neutral-50);
    font-family: var(--font-family-heading);
    font-size: var(--font-size-40);
    display: flex;
    line-height: 1;
    padding: 0.75rem 0.75rem 0.8rem 0.75rem;
    text-decoration: none;
    font-weight: 400;
  }

.c-navigation__item a:hover {
      color: var(--color-neutral-90);
    }

.t-dark .c-navigation__item.c-navigation__item--selected a {
  color: var(--color-neutral-0);
}

.t-dark .c-navigation__item a {
  color: color-mod(var(--color-neutral-0) alpha(60%));
}

.t-dark .c-navigation__item a:hover {
    color: color-mod(var(--color-neutral-0) alpha(40%));
  }

/*
 * Link like social media
 */

.c-navigation__links {
  display: flex;
  gap: 8px;
  list-style: none;
}

@media (min-width: 750px) {

.c-navigation__links {
    opacity: 0;
    padding-right: 16px;
    animation: c-navigation 300ms cubic-bezier(0, 0, 0, 1) forwards 450ms
}
  }

.c-navigation__links-item a {
    padding: 0.25rem;
    display: block;
  }

.c-navigation__links-item a:hover svg {
      fill: var(--color-neutral-90);
    }

.c-navigation__links-item svg {
    height: 20px;
    width: 20px;
    fill: var(--color-neutral-50);
    transition: fill 100ms ease-out;
  }

.t-dark .c-navigation__links-item a:hover svg {
    fill: color-mod(var(--color-neutral-0) alpha(20%));
  }

.t-dark .c-navigation__links-item svg {
    fill: color-mod(var(--color-neutral-0) alpha(40%));
  }

/*------------------------------------*\
  #COMPONENTS-PARAGRAPH
\*------------------------------------*/

p,
.c-paragraph,
.u-content p {
  line-height: var(--line-height-30);
  margin: var(--space-stack-30);
  font-size: var(--font-size-40);
  color: var(--color-neutral-90);
}

@media (min-width: 750px) {

p,
.c-paragraph,
.u-content p {
    /* margin: var(--space-stack-40); */
}
  }

.t-dark p,
.t-dark .c-paragraph,
.t-dark .u-content p {
  color: var(--color-neutral-0);
}

.c-paragraph--lead,
.u-content .c-paragraph--lead {
  color: var(--color-primary-50);
  font-size: 22px;
  font-family: var(--font-family-b);
}

@media (min-width: 750px) {

.c-paragraph--lead,
.u-content .c-paragraph--lead {
    font-size: 26px;
    line-height: 1.75em
}
  }

.t-dark .c-paragraph--lead,
.t-dark .u-content .c-paragraph--lead {
  color: var(--color-primary-30);
}

.c-paragraph--soft,
.u-content .c-paragraph--soft {
  color: var(--color-neutral-50);
  font-size: var(--font-size-default);
  line-height: 1.5em;
  /* margin: var(--space-stack-30); */
}

.t-dark .c-paragraph--soft,
.t-dark .u-content .c-paragraph--soft {
  color: color-mod(var(--color-neutral-0) alpha(50%));
}

@keyframes c-paragraph--animation {
  0% {
    opacity: 0;
    transform: translateX(200px);
  }

  100% {
    opacity: 1;
    transform: translateX(0);
  }
}

@media (min-width: 750px) {

.c-paragraph--animation {
    opacity: 0;
    animation: c-paragraph--animation 600ms cubic-bezier(0, 0, 0, 1) forwards
      200ms
}
  }

/*------------------------------------*\
  #BASE-QUOTE
\*------------------------------------*/

.c-blockquote {
  margin: var(--space-stack-default);
  padding-left: 1rem;
  margin-left: calc(1rem - 2px);
  border-left: 2px solid var(--color-neutral-30);
  position: relative;
  quotes: none;
}

@media (min-width: 750px) {

.c-blockquote {
    margin-left: calc(2rem - 2px)
}
  }

.c-blockquote p {
    color: var(--color-neutral-60);
    margin: var(--space-stack-30);
  }

.c-blockquote cite {
    font-size: var(--font-size-20);
    font-style: normal;
  }

.c-blockquote cite:before {
      content: "\2014 \0020";
    }

.t-dark blockquote,
.l-wrapper--neutral-70 blockquote {
  border-left-color: color-mod(var(--color-neutral-0) alpha(10%));
}

