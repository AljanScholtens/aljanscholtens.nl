}
  }

@media (min-width: 1000px) {

.c-gallery__item--40 {
    height: 35vw;
    max-height: 22rem;
    width: 40%
}
  }

@media (min-width: 750px) {

.c-gallery__item--50 {
    height: 30vw;
    width: 50%
}
  }

@media (min-width: 1000px) {

.c-gallery__item--50 {
    height: 30vw;
    max-height: 22rem;
    width: 50%
}
  }

@media (min-width: 750px) {

.c-gallery__item--60 {
